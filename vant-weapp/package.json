{
  "name": "vant-weapp",
  "version": "0.5.14",
  "author": "youzan",
  "license": "MIT",
  "miniprogram": "dist",
  "description": "轻量、可靠的小程序 UI 组件库",
  "scripts": {
    "dev": "node build/dev.js",
    "lint": "eslint ./packages --ext .js",
    "release": "sh build/release.sh",
    "build:lib": "yarn && rm -rf dist && rm -rf lib && NODE_ENV=production node build/compiler.js",
    "build:site": "rm -rf docs/dist && cross-env NODE_ENV=production webpack --config build/webpack.doc.prd.js && gh-pages -d docs/dist",
    "build:changelog": "vant-doc changelog --tag v0.5.0 changelog.generated.md"
  },
  "files": [
    "dist",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/youzan/vant-weapp.git"
  },
  "homepage": "https://github.com/youzan/vant-weapp#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@vant/doc": "^2.3.2",
    "@vant/icons": "1.1.8",
    "@vant/markdown-loader": "^2.0.0",
    "autoprefixer": "^9.6.0",
    "babel-loader": "8.0.6",
    "cross-env": "^5.1.4",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-plugin-vue-libs": "^3.0.0",
    "gh-pages": "^2.0.1",
    "gulp": "^4.0.2",
    "gulp-insert": "^0.5.0",
    "gulp-less": "^4.0.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-typescript": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "miniprogram-api-typings": "2.4.2-2",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "style-loader": "^0.23.1",
    "typescript": "^3.5.2",
    "vue": "2.6.10",
    "vue-loader": "^15.4.2",
    "vue-router": "^3.0.6",
    "vue-template-compiler": "2.6.10",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "webpack-serve": "^2.0.3"
  }
}
